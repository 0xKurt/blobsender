# Private key of the deployer account (without 0x prefix)
# WARNING: Never commit your actual private key to version control!
PRIVATE_KEY=your_private_key_here

# Worker address for the Escrow contract
# This address will be set as the worker in the constructor
WORKER_ADDRESS=0x0000000000000000000000000000000000000000

# RPC URLs for Ethereum networks
# Get your RPC URL from providers like Alchemy, Infura, or QuickNode
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Etherscan API keys for contract verification
# Get your API key from https://etherscan.io/apis
ETHERSCAN_API_KEY=your_etherscan_api_key_here
SEPOLIA_ETHERSCAN_API_KEY=your_sepolia_etherscan_api_key_here

